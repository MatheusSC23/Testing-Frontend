{
  "extends": [
    "eslint: recommended",
    "plugin: import/errors",
    "plugin: react/recommended",
    "plugin:jsx-a11y/recommended",
    "plugin: react-hooks/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["react", "import", "jsx-ally"],
  "env": {
    "es6": true,
    "browser": true,
    "jest": true,
    "node": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
